{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aduka-data-factory"
		},
		"AzureDataLakeStorageLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageLinkedService'"
		},
		"VMSDatabaseLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'VMSDatabaseLinkedService'"
		},
		"AzureDataLakeStorageLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adukastorageaccount.dfs.core.windows.net"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/VMS_Database')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "VMSDatabaseLinkedService",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "date_of_sale",
						"type": "date"
					},
					{
						"name": "street",
						"type": "varchar"
					},
					{
						"name": "area",
						"type": "varchar"
					},
					{
						"name": "city",
						"type": "varchar"
					},
					{
						"name": "district",
						"type": "varchar"
					},
					{
						"name": "state_place",
						"type": "varchar"
					},
					{
						"name": "sale_price",
						"type": "int",
						"precision": 10
					},
					{
						"name": "building_type",
						"type": "varchar"
					},
					{
						"name": "lot_status",
						"type": "varchar"
					},
					{
						"name": "lot_type",
						"type": "varchar"
					},
					{
						"name": "ownership_type",
						"type": "varchar"
					},
					{
						"name": "lease_type",
						"type": "varchar"
					},
					{
						"name": "rooms",
						"type": "int",
						"precision": 10
					},
					{
						"name": "floors",
						"type": "decimal",
						"precision": 5,
						"scale": 2
					},
					{
						"name": "builtup_area_sqft",
						"type": "decimal",
						"precision": 7,
						"scale": 2
					},
					{
						"name": "price_psf",
						"type": "decimal",
						"precision": 7,
						"scale": 2
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "highrise"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/VMSDatabaseLinkedService')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/hr100')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureDataLakeStorageLinkedService",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "hr_100.csv",
						"folderPath": "test",
						"fileSystem": "vms-container"
					},
					"columnDelimiter": ",",
					"escapeChar": "\"",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "date_of_sale",
						"type": "String"
					},
					{
						"name": "street",
						"type": "String"
					},
					{
						"name": "area",
						"type": "String"
					},
					{
						"name": "city",
						"type": "String"
					},
					{
						"name": "district",
						"type": "String"
					},
					{
						"name": "sale_price",
						"type": "String"
					},
					{
						"name": "price_psf",
						"type": "String"
					},
					{
						"name": "builtup_area_sqft",
						"type": "String"
					},
					{
						"name": "building_type",
						"type": "String"
					},
					{
						"name": "floors",
						"type": "String"
					},
					{
						"name": "lot_status",
						"type": "String"
					},
					{
						"name": "lot_type",
						"type": "String"
					},
					{
						"name": "ownership_type",
						"type": "String"
					},
					{
						"name": "lease_type",
						"type": "String"
					},
					{
						"name": "rooms",
						"type": "String"
					},
					{
						"name": "state",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureDataLakeStorageLinkedService')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorageLinkedService')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Linked service for ADLS Gen2",
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorageLinkedService_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AzureDataLakeStorageLinkedService_accountKey')]"
					}
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/VMSDatabaseLinkedService')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Linked service for VMS database in Aduka SQL Server",
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('VMSDatabaseLinkedService_connectionString')]"
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AutoResolveIntegrationRuntime')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "AutoResolve",
						"dataFlowProperties": {
							"computeType": "General",
							"coreCount": 8,
							"timeToLive": 0
						}
					}
				},
				"managedVirtualNetwork": {
					"type": "ManagedVirtualNetworkReference",
					"referenceName": "default"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/managedVirtualNetworks/default')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/default')]",
			"type": "Microsoft.DataFactory/factories/managedVirtualNetworks",
			"apiVersion": "2018-06-01",
			"properties": {
				"preventDataExfiltration": false
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/CopyCSV2SQL')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Copy CSV files from dashboard folder into SQL database",
				"activities": [
					{
						"name": "ETL",
						"description": "ETL from data lake into VMS database",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "vms-container/test/hr_100.csv"
							},
							{
								"name": "Destination",
								"value": "dbo.highrise"
							}
						],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "AzureSqlSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "date_of_sale",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "date_of_sale",
											"type": "DateTime",
											"physicalType": "date"
										}
									},
									{
										"source": {
											"name": "street",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "street",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "area",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "area",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "city",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "city",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "district",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "district",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "state",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "state_place",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "sale_price",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "sale_price",
											"type": "Int32",
											"physicalType": "int"
										}
									},
									{
										"source": {
											"name": "price_psf",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "price_psf",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 5
										}
									},
									{
										"source": {
											"name": "builtup_area_sqft",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "builtup_area_sqft",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 5
										}
									},
									{
										"source": {
											"name": "building_type",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "building_type",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "floors",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "floors",
											"type": "Decimal",
											"physicalType": "decimal",
											"scale": 2,
											"precision": 3
										}
									},
									{
										"source": {
											"name": "lot_status",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "lot_status",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "lot_type",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "lot_type",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "ownership_type",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "ownership_type",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "lease_type",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "lease_type",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "rooms",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "rooms",
											"type": "Int32",
											"physicalType": "int"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "hr100",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "VMS_Database",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/hr100')]",
				"[concat(variables('factoryId'), '/datasets/VMS_Database')]"
			]
		}
	]
}